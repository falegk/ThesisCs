<div class="widget widget-table action-table">
  <div class="widget-header"> <i class="icon-th-list"></i>
    <h3><%= widgetTitle ? widgetTitle : 'Πρόσφατα ενεργές πτυχιακές!' %>  </h3>
  </div>
  <!-- /widget-header -->
  <div class="widget-content">
    <% if activeProjects.count == 0 %>
       <div class="fc-content-center">Δεν υπάρχουν αποτελέσματα.</div>
    <% else %>
    <table class="table table-striped table-bordered">
      <thead>
      <tr>
        <th> Τιτλος </th>
        <th> Φοιτητης/ες</th>
      </tr>
      </thead>
      <tbody>
          <%= render partial: 'teachers/share/dissertations_table_widget_rows',
                     locals: {activeProject: activeProjects } %>
      <% end %>
      </tbody>
    </table>

  </div>
  <!-- /widget-content -->
</div>